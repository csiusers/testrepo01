                    <optionSet id="Localization Directive (LocRule) Settings" displayName="Localization Directive (LocRule) Settings" helpText="Use these settings to control how Localization Directives are handled for specific resources.">
                        <optionSet id="Adaptation Rule" displayName="Adaptation Rule" helpText="Use the Adaptation rule settings to identify and correctly localize strings based on content in the map file. Do not change these settings without the help of the LocStudio team.">
                            <optionSet id="Font Settings" displayName="Font Settings" helpText="Use these settings to locate strings with a string category of FontName and correctly localize the strings using content in the Map file with the ID of Fonts.">
                                <option id="\LocRuleAgent\AdaptationRule\AdaptationRule01\Enabled" displayName="Enabled" helpText="To validate strings per this rule, select On. To stop validation with this rule, select Off.">
                                    <boolean defaultValue="1" currentValue="0" />
                                </option>
                            </optionSet>
                        </optionSet>
                        <optionSet id="Placeholder Rule" displayName="Placeholder Rule" helpText="Use these settings to mark placeholder text that must exist in the target when it exits in the source. Do not change these settings without help from the LocStudio team.">
                            <optionSet id="Win32 Message Table Settings" displayName="Win32 Message Table Settings" helpText="Use these settings to set up placeholders when using message table settings with the Win32 parser.">
                                <option id="\LocRuleAgent\PlaceholderRule\PlaceholderRule02\AlwaysPlaceEscapeChars" displayName="Always Place Escape Chars" helpText="Select On to place an invalid character constraint even if no placeholder is found, and to be notified when escape characters are not used.">
                                    <boolean defaultValue="1" currentValue="0" />
                                </option>
                                <option id="\LocRuleAgent\PlaceholderRule\PlaceholderRule02\RegEx" displayName="RegEx" helpText="If you are creating a new version of a rule using a placeholder, enter a regular expression to identify information or characters in the string. Otherwise, do not change this setting.">
                                    <string defaultValue="%[0-9][0-9]?(![-+0 #]?\d*\.?\d*(h|l|I|I32|I64)?[\dcCdiouxXeEfgGnpsS]!)?" currentValue="%(([1-9][0-9]?(!([-+]?#?(\d+|\*)?\.?(\d+|\*)?(h|l|I|w|I32|I64)?[cCdiouxXGnpsS])!)?)|0|n| |\.|!|r|t)" />
                                </option>
                            </optionSet>
                            <optionSet id="PrintF Variable Settings" displayName="PrintF Variable Settings" helpText="Use these settings to set up placeholders when using PrintF variables with the Win32 parser.">
                                <option id="\LocRuleAgent\PlaceholderRule\PlaceholderRule01\AlwaysPlaceEscapeChars" displayName="Always Place Escape Chars" helpText="Select On to place an invalid character constraint even if no placeholder is found, and to be notified when escape characters are not used.">
                                    <boolean defaultValue="1" currentValue="0" />
                                </option>
                                <option id="\LocRuleAgent\PlaceholderRule\PlaceholderRule01\RegEx" displayName="RegEx" helpText="If you are creating a new version of a rule using a placeholder, enter a regular expression to identify information or characters in the string. Otherwise, do not change this setting.">
                                    <string defaultValue="%[-+]?[0-9]*\.?[0-9]*(h|l|I|I32|I64)?[cCdiouxXeEfgGnpsS]" currentValue="%([-+]?#?\d*\.?\d*(h|l|I|w|I32|I64)?[cCdiouxXeEfgGnpsS])" />
                                </option>
                            </optionSet>
                            <optionSet id="Placeholder Rule Placeholder 08" displayName="Placeholder Rule Placeholder 08" helpText="Use these settings to create additional instances of the Placeholder rule.">
                                <option id="\LocRuleAgent\PlaceholderRule\PlaceholderRule08\EscapeChars" displayName="EscapeChars" helpText="Enter characters that are not valid and their escape sequences.">
                                    <stringList>
                                        <defaultList />
                                        <currentList>
                                            <item string="\ = \\" itemID="0" />
                                            <item string="&quot; = \&quot;" itemID="1" />
                                        </currentList>
                                    </stringList>
                                </option>
                                <option id="\LocRuleAgent\PlaceholderRule\PlaceholderRule08\Enabled" displayName="Enabled" helpText="To validate strings per this rule, select On. To stop validation with this rule, select Off.">
                                    <boolean defaultValue="0" currentValue="1" />
                                </option>
                                <option id="\LocRuleAgent\PlaceholderRule\PlaceholderRule08\DocumentType" displayName="Document Type" helpText="Enter the parser IDs this rule will affect. If left blank, files parsed by any parser are affected by this rule.">
