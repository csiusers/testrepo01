                                <option id="\LocRuleAgent\SpecialCharsRule\SpecialCharsRule03\DocumentType" displayName="Document Type" helpText="Enter the parser IDs this rule will affect. If left blank, files parsed by any parser are affected by this rule.">
                                    <stringList>
                                        <defaultList />
                                        <currentList>
